{
  "name": "automationexercise-wdio-cucumber-ts",
  "version": "1.0.0",
  "description": "UI regression framework for automationexercise.com using WebdriverIO + TypeScript",
  "license": "ISC",
  "author": "Ben Azeta",
  "type": "commonjs",
  "main": "index.js",
  "keywords": [
    "wdio",
    "cucumber",
    "e2e",
    "tests"
  ],
  "scripts": {
    "test": "npm run wdio && npm run allure:generate && npm run allure:open",
    "wdio": "wdio run ./wdio.conf.ts",
    "allure:generate": "npx allure generate allure-results --clean -o allure-report",
    "allure:open": "npx allure open allure-report",
    "test:smoke": "cross-env TAGS='@smoke and not @wip' wdio run wdio.conf.ts",
    "test:regression": "cross-env TAGS='@regression and not @wip' wdio run wdio.conf.ts",
    "test:auth": "cross-env TAGS='@auth and not @wip' wdio run wdio.conf.ts",
    "test:checkout": "cross-env TAGS='@checkout and not @wip' wdio run wdio.conf.ts",
    "test:headed": "cross-env HEADLESS=false wdio run wdio.conf.ts",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "loginLogout": "npm run wdio -- --spec src/features/Login-and-Logout/AE-AUTH-001-Login-Logout.feature",
    "registerUser": "npm run wdio -- --spec src/features/Register-User-To-Delete-Account/Register-User.feature",
    "searchProduct": "npm run wdio -- --spec src/features/Search-Product-Validate-Results-Add-to-Cart/AE-SRCH-001-Search-Product.feature",
    "placeOrder": "npm run wdio -- --spec src/features/Place-Order-And-Login/AE-CHK-001-Place-Order.feature"
  },
  "devDependencies": {
    "@types/chai": "^4.3.20",
    "@types/mssql": "^7.1.5",
    "@types/node": "^25.2.1",
    "@types/supertest": "^2.0.16",
    "@wdio/allure-reporter": "^9.19.1",
    "@wdio/cli": "^9.23.3",
    "@wdio/cucumber-framework": "^9.23.3",
    "@wdio/globals": "^9.23.0",
    "@wdio/local-runner": "^9.23.3",
    "@wdio/spec-reporter": "^9.23.3",
    "allure-commandline": "^2.27.0",
    "chai": "^4.5.0",
    "chromedriver": "^139.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "expect-webdriverio": "^5.6.4",
    "geckodriver": "^6.1.0",
    "prettier": "^3.4.2",
    "supertest": "^6.3.4",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "winston": "^3.17.0"
  },
  "dependencies": {
    "webdriverio": "^9.23.3"
  }
}
